{
  "name": "@tarsilla/eslint-config",
  "version": "0.0.51",
  "engines": {
    "node": ">=22"
  },
  "main": "./lib/index.mjs",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    },
    "./library": {
      "types": "./lib/library/index.d.ts",
      "require": "./lib/library/index.cjs",
      "import": "./lib/library/index.mjs",
      "default": "./lib/library/index.mjs"
    },
    "./react": {
      "types": "./lib/react/index.d.ts",
      "require": "./lib/react/index.cjs",
      "import": "./lib/react/index.mjs",
      "default": "./lib/react/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
  "files": [
    "lib/*",
    "src/*",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc": "tsc --noEmit",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run lint && npm run tsc && rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access public",
    "prepare": "husky"
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "dependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.1",
    "typescript-eslint": "^8.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/eslint__js": "^8.42.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "rollup": "^4.34.8",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "semantic-release": "^24.2.3",
    "typescript": "^5.7.3"
  }
}
